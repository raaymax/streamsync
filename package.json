{
  "name": "Streamsync",
  "version": "1.0.0",
  "description": "",
  "workspaces": [
    "cli",
    "ui",
    "e2e",
    "docs"
  ],
  "scripts": {
    "docs:build": "npm run -w streamsync-docs docs:build",
    "docs:dev": "npm run -w streamsync-docs docs:dev",
    "docs:preview": "npm run -w streamsync-docs docs:preview",
    "build": "turbo run build",
    "custom.build": "npm run -w streamsync-ui custom.build",
    "e2e": "turbo run e2e",
    "e2e:ui": "npm run -w streamsync-e2e test:ui",
    "e2e:firefox": "turbo e2e:firefox",
    "e2e:chromium": "turbo e2e:chromium",
    "e2e:webkit": "turbo e2e:webkit",
    "lint": "turbo run lint",
    "lint:ci": "npm run -w streamsync-ui lint:ci",
		"ci": "turbo run build lint test",
    "dev": "npm run -w streamsync-ui dev"
  },
  "dependencies": {
    "turbo": "1.12.4"
  }
}
